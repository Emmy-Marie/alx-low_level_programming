#include "main.h"
#include <stdio.h>
#include <stdlib.h>

/**
 * main - Entry point - A program that multiplies two numbers
 * @argc: Argument count
 * @argv: Argument vector
 * Return: 0 for success and 1 if otherwise
 */

int main(int argc, char *argv[])
{
	int sum;

	if (argc == 3)
	{
		sum = atoi(argv[1]) * atoi(argv[2]);
		printf("%d\n", sum);
	}
	else
	{
		printf("\nError\n");
		return (1);
	}
	return (0);
}
