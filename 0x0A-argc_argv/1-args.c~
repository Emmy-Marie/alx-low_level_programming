#include "main.h"
#include <stdio.h>

/**
 * main - Entry point - A program that prints the
 *        number of arguments passed into it
 * @argc: Argument count
 * @argv: Argument vector
 * Return: 0 for success
 */

int main(int argc, char *argv[])
{
	int count, num;
	(void)argv;

	count = 0;
	if (argc > 1)
	{
		for (count = 0; count < argc; count++)
		{
			num = count + 1;
		}
		printf("%d\n", num);
	}
	else
	{
		printf("%d\n", count);
	}
	return (0);
}
