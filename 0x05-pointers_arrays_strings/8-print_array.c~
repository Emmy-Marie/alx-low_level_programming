#include "main.h"
#include <stdio.h>

/**
 * print_array - Print n elements of an array of integers
 * @a: Integer pointer variable
 * @n: number of elements of an integer array
 */

void print_array(int *a, int n)
{
	int i;

	for (i = 0; i < n; i++)
	{
		printf("%d", a[i]);

		if (i != n - 1)
		{
			printf(", ");
		}
	}
	_printf("\n");
}
